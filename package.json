{
  "name": "url-shortener-client",
  "version": "1.0.0",
  "description": "Client for url-shortener",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "test": "mocha src --opts mocha.opts",
    "test:watch": "npm test -- -w",
    "test:coverage": "NODE_ENV=test nyc npm test",
    "clean": "rimraf dist/*",
    "dev": "webpack-dev-server --config webpack.dev.config.js",
    "build": "npm run clean && NODE_ENV=production webpack -p --config webpack.prod.config.js --progress --profile --colors",
    "lint": "eslint .",
    "prestart": "mongod --fork --logpath /dev/null",
    "stopdb": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "start": "nodemon --exec babel-node server.js"
  },
  "author": "Graeme Cottrell",
  "dependencies": {
    "assert": "^1.4.1",
    "axios": "^0.17.0",
    "babel-polyfill": "^6.16.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "express": "^4.14.0",
    "http-status-codes": "^1.0.6",
    "lodash": "^4.16.2",
    "mongodb": "^2.2.33",
    "nock": "^8.0.0",
    "prop-types": "^15.6.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "string-hash": "^1.1.3",
    "superagent": "^2.3.0",
    "superagent-es6-promise": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.5.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "nyc": "^8.3.0",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^1.3.0",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-merge": "^0.15.0"
  },
  "nyc": {
    "exclude": [
      "src/**/*.spec.js",
      "test/**"
    ],
    "all": true
  }
}
